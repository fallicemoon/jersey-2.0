<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.jersey.commodity.model.CommodityAttrMappingVO" table="commodity_attr_mapping" >
        
        <id name="commodityAttrId" column="commodity_type_id">
            <generator class="identity"/>
        </id>
        
        <set name="commoditys" table="commodity" lazy="false" order-by="commodityId asc">
        	<key><column name="commodity_type_id"></column></key>
        	<one-to-many class="com.jersey.commodity.model.CommodityVO" />
        </set>
        
        <set name="commodityAttrs" table="commodity_attr" lazy="false" order-by="commodityAttrId asc">
        	<key><column name="commodity_type_id"></column></key>
        	<one-to-many class="com.jersey.userConfig.model.CommodityAttrVO" />
        </set>

    	<property name="createTime" column="create_time" />
    	<property name="lastModifyTime" column="last_modify_time" />
    </class>
</hibernate-mapping>

